program circle;
var
	{Missing right parenthesis}
	r, a, p, b : real; 
	i, j : integer;	
begin
	r := 8;
	p := 0;
	a := 0;
	j := -1;
	if r > 5 then
	  a := (((3.14) * r ) + p + a * r;
	if (a < 100) then
	    p := 2 * 3.14 * b; 
	{Display the results}
	writeln  'The output results are ' , r, p, b);
end.
